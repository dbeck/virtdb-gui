@font-face
  font-family: 'Glyphicons Halflings';
  src: url('../libs/glyphicons-halflings-regular.eot');
  src: url('../libs/glyphicons-halflings-regular.eot?#iefix') format('embedded-opentype'), url('../libs/glyphicons-halflings-regular.woff') format('woff'), url('../libs/glyphicons-halflings-regular.ttf') format('truetype'), url('../fonts/glyphicons-halflings-regular.svg#glyphicons_halflingsregular') format('svg');

.main
    color blue

.videowrapper
    position relative
    padding-bottom 56.25%
    padding-top 25p
    height 0

.videowrapper iframe
    position absolute
    top 0
    left 0
    width 100%
    height 100%

.modal-backdrop
    z-index -1

.mermaid-label
