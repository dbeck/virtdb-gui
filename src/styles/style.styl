// Hacks for increasing freelancer template ul-s
ul
    font-size 20px

h1, h2, h3, h4, h5
    color #0071bc

h3, h4, h5
    text-transform none

a.btn-social
    color #0071bc

p
    text-align justify

.ng-invalid
    border-color red
